  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", " ", "=", "1"}], " ", ",", " ", 
    RowBox[{"i", "\[LessEqual]", " ", "Nmax"}], ",", " ", 
    RowBox[{"i", "++"}], ",", "\n", 
    RowBox[{
     RowBox[{"x2", " ", "=", " ", "\n", 
      RowBox[{"N", "[", 
       RowBox[{"x1", "-", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"f", "[", "x", "]"}], "/.", 
           RowBox[{"x", "\[Rule]", "x1"}]}], ")"}], "*", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"x1", "-", "x0"}], ")"}], "/", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"f", "[", "x", "]"}], "/.", 
              RowBox[{"x", "\[Rule]", "x1"}]}], ")"}], "-", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"f", "[", "x", "]"}], "/.", 
              RowBox[{"x", "\[Rule]", "x0"}]}], ")"}]}], ")"}]}]}]}], "]"}]}],
      ";", "\n", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", 
         RowBox[{"x1", "-", "x2"}], "]"}], "<", "eps"}], ",", 
       RowBox[{"Return", "[", "x2", "]"}], ",", 
       RowBox[{
        RowBox[{"x0", " ", "=", " ", "x1"}], ";", 
        RowBox[{"x1", " ", "=", " ", "x2"}]}]}], "]"}], ";", "\n", 
     RowBox[{"Print", "[", 
