  RowBox[{"Print", "[", 
   RowBox[{"\"\<epsilon = \>\"", ",", "eps"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], " ", ":=", " ", 
   RowBox[{
    RowBox[{"Cos", "[", "x", "]"}], "-", 
    RowBox[{"x", "*", 
     RowBox[{"Exp", "[", "x", "]"}]}]}]}], ";"}], "\n", 
