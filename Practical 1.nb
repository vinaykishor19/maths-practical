Cell["Bisection Method:For Parameters Taken from user:-", "Subsubsection",
 ExpressionUUID -> "08494fac-9bf2-43e0-b723-ffe9221499e9"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"x0", " ", "=", " ", 
   RowBox[{"Input", "[", "\"\<Enter first number :\>\"", "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"x1", " ", "=", " ", 
   RowBox[{"Input", "[", "\"\<Enter second number :\>\"", "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"Nmax", " ", "=", 
   RowBox[{
   "Input", "[", "\"\<Enter maximum number of iterations :\>\"", "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"eps", " ", "=", " ", 
   RowBox[{
   "Input", "[", "\"\<Enter a value of convergence parameter :\>\"", "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<x0 = \>\"", ",", "x0"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<x1 = \>\"", ",", "x1"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<Nmax = \>\"", ",", "Nmax"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<epsilon = \>\"", ",", "eps"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], " ", ":=", " ", 
   RowBox[{"Cos", "[", "x", "]"}]}], ";"}], "\n", 
 RowBox[{"If", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"N", "[", 
     RowBox[{
      RowBox[{"f", "[", "x0", "]"}], "*", 
      RowBox[{"f", "[", "x1", "]"}]}], "]"}], ">", "0"}], ",", "\n", 
   RowBox[{
   "Print", "[", 
    "\"\<Yours values do not satisfy the IVP so change the value.\>\"", "]"}],
    ",", "\n", 
   RowBox[{
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
      RowBox[{"i", "\[LessEqual]", " ", "Nmax"}], ",", " ", 
      RowBox[{"i", "++"}], ",", " ", 
      RowBox[{
       RowBox[{"m", " ", "=", " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"x0", "+", "x1"}], ")"}], "/", "2"}]}], ";", "\n", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"x1", "-", "x0"}], ")"}], "/", "2"}], "]"}], "<", 
          "eps"}], ",", 
         RowBox[{"Return", "[", "m", "]"}], ",", "\n", 
         RowBox[{
          RowBox[{"Print", "[", 
           RowBox[{
           "i", ",", "\"\< th iteration value is : \>\"", ",", " ", "m"}], 
           "]"}], ";", "\n", 
          RowBox[{"Print", "[", 
           RowBox[{
           "\"\<Estimated error in \>\"", ",", " ", "i", ",", " ", 
            "\"\<th iteration is : \>\"", ",", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"x1", "-", "x0"}], ")"}], "/", "2"}]}], "]"}], ";", 
          "\n", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"f", "[", "m", "]"}], "*", 
              RowBox[{"f", "[", "x1", "]"}]}], ">", "0"}], ",", " ", 
            RowBox[{"x1", " ", "=", " ", "m"}], " ", ",", " ", 
            RowBox[{"x0", " ", "=", " ", "m"}]}], "]"}]}]}], "]"}]}]}], "]"}],
     ";", "\n", 
    RowBox[{
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Root is : \>\"", ",", " ", "m"}], "]"}], "\n", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<Estimated error in \>\"", ",", " ", "i", ",", " ", 
       "\"\<th iteration is :\>\"", ",", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"x1", "-", "x0"}], ")"}], "/", "2"}]}], "]"}]}]}]}], 
  "]"}], "\n", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"f", "[", "x", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"-", "1"}], ",", "3"}], "}"}], ",", " ", 
   RowBox[{"PlotRange", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"-", "1"}], ",", "1"}], "}"}]}], ",", "\n", 
   RowBox[{"PlotStyle", "\[Rule]", "Red"}], ",", " ", 
   RowBox[{"PlotLabel", "\[Rule]", 
    RowBox[{"\"\<f[x]=\>\"", " ", 
     RowBox[{"f", "[", "x", "]"}]}]}], ",", " ", 
   RowBox[{"AxesLabel", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      RowBox[{"f", "[", "x", "]"}]}], "}"}]}]}], "]"}]}], "Input",
 ExpressionUUID -> "78d42ef2-7dd3-41be-aa2a-738d12dc7f0f"],

