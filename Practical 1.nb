    RowBox[{"x", "^", "3"}], " ", "-", " ", 
    RowBox[{"5", "x"}], " ", "+", " ", "1"}]}], ";"}], "\n", 
 RowBox[{"If", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"N", "[", 
     RowBox[{
      RowBox[{"f", "[", "x0", "]"}], "*", 
      RowBox[{"f", "[", "x1", "]"}]}], "]"}], ">", "0"}], ",", "\n", 
   RowBox[{
   "Print", "[", 
    "\"\<Yours values do not satisfy the IVP so change the value.\>\"", "]"}],
    ",", "\n", 
   RowBox[{
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
      RowBox[{"i", "\[LessEqual]", " ", "Nmax"}], ",", " ", 
      RowBox[{"i", "++"}], ",", " ", 
      RowBox[{
       RowBox[{"m", " ", "=", " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"x0", "+", "x1"}], ")"}], "/", "2"}]}], ";", "\n", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"x1", "-", "x0"}], ")"}], "/", "2"}], "]"}], "<", 
          "eps"}], ",", 
         RowBox[{"Return", "[", "m", "]"}], ",", "\n", 
         RowBox[{
          RowBox[{"Print", "[", 
           RowBox[{
           "i", ",", "\"\< th iteration value is : \>\"", ",", " ", "m"}], 
           "]"}], ";", "\n", 
          RowBox[{"Print", "[", 
           RowBox[{
           "\"\<Estimated error in \>\"", ",", " ", "i", ",", " ", 
            "\"\<th iteration is : \>\"", ",", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"x1", "-", "x0"}], ")"}], "/", "2"}]}], "]"}], ";", 
          "\n", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"f", "[", "m", "]"}], "*", 
              RowBox[{"f", "[", "x1", "]"}]}], ">", "0"}], ",", " ", 
            RowBox[{"x1", " ", "=", " ", "m"}], " ", ",", " ", 
            RowBox[{"x0", " ", "=", " ", "m"}]}], "]"}]}]}], "]"}]}]}], "]"}],
     ";", "\n", 
    RowBox[{
     RowBox[{"Print", "[", 
