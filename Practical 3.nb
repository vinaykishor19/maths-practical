  RowBox[{"Print", "[", 
   RowBox[{"\"\<epsilon = \>\"", ",", "eps"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], " ", ":=", " ", 
   RowBox[{"Cos", "[", "x", "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<f[x] := \>\"", ",", 
    RowBox[{"f", "[", "x", "]"}]}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<f'[x]:=\>\"", ",", 
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{"f", "[", "x", "]"}], ",", "x"}], "]"}]}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", " ", "=", "1"}], ",", " ", 
    RowBox[{"i", "\[LessEqual]", "Nmax"}], ",", 
    RowBox[{"i", "++"}], ",", 
    RowBox[{
     RowBox[{"x1", " ", "=", " ", 
      RowBox[{"N", "[", 
       RowBox[{"x0", "-", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"f", "[", "x", "]"}], "/.", 
           RowBox[{"x", "\[Rule]", "x0"}]}], ")"}], "/", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"D", "[", 
            RowBox[{
             RowBox[{"f", "[", "x", "]"}], ",", "x"}], "]"}], "/.", 
           RowBox[{"x", "\[Rule]", "x0"}]}], ")"}]}]}], "]"}]}], ";", "\n", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Abs", "[", 
         RowBox[{"x1", "-", "x0"}], "]"}], "<", "eps"}], ",", 
       RowBox[{"Return", "[", "x1", "]"}], ",", 
       RowBox[{
        RowBox[{"x0p", "=", "x0"}], ";", 
        RowBox[{"x0", "=", "x1"}]}]}], "]"}], ";", "\n", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<In \>\"", ",", "i", ",", "\n", 
       "\"\<th Number of iterations the approximation to root is :\>\"", ",", 
       "x1"}], "]"}], ";", "\n", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Estimated error is :\>\"", ",", 
       RowBox[{"Abs", "[", 
        RowBox[{"x1", "-", "x0p"}], "]"}]}], "]"}]}]}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<The final approximation of root is :\>\"", ",", "x1"}], "]"}],
   ";"}], "\n", 
